# Agent Environment Configuration
# Copy this file to .env and modify as needed
# Priority: Environment Variables > This File > Code Defaults

# =============================================================================
# LLM Configuration
# =============================================================================

# Provider: dashscope (Aliyun) | openai | ollama
LLM_PROVIDER=dashscope

# Aliyun DashScope (default - OpenAI compatible)
LLM_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
LLM_MODEL=qwen-plus
LLM_API_KEY=sk-2def4a1a570a456898ae847090a5ed97

# Alternative: OpenAI
# LLM_BASE_URL=https://api.openai.com/v1
# LLM_MODEL=gpt-4o-mini
# LLM_API_KEY=sk-xxx

# Alternative: Ollama (local)
# LLM_PROVIDER=ollama
# LLM_BASE_URL=http://localhost:11434
# LLM_MODEL=qwen2.5:7b

# Request settings
LLM_TIMEOUT=120
LLM_MAX_TOKENS=2048
LLM_TEMPERATURE=0.7

# =============================================================================
# Agent Service Configuration
# =============================================================================

# Agent URLs (for main service routing)
AGENT_TRAVEL_ASSISTANT_URL=http://localhost:9101
AGENT_CHARGING_MANAGER_URL=http://localhost:9002
AGENT_BILLING_ADVISOR_URL=http://localhost:9003
AGENT_EMERGENCY_SUPPORT_URL=http://localhost:9004
AGENT_DATA_ANALYST_URL=http://localhost:9005
AGENT_MAINTENANCE_EXPERT_URL=http://localhost:9006
AGENT_ENERGY_ADVISOR_URL=http://localhost:9007
AGENT_SCHEDULING_ADVISOR_URL=http://localhost:9008

# Agent host binding
AGENT_HOST=0.0.0.0

# =============================================================================
# A2A Configuration
# =============================================================================
A2A_ENABLED=true
A2A_PROTOCOL_VERSION=0.3.0
A2A_TIMEOUT=180.0

# =============================================================================
# Nacos Service Registration Configuration
# =============================================================================
NACOS_ENABLED=true
NACOS_SERVER_ADDRESSES=localhost:8848
NACOS_NAMESPACE=public
NACOS_GROUP=DEFAULT_GROUP
NACOS_USERNAME=
NACOS_PASSWORD=
NACOS_CLUSTER_NAME=DEFAULT
NACOS_HEARTBEAT_INTERVAL=5
