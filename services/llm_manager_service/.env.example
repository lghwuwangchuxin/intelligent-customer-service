LLM_PROVIDER=ollama
LLM_MODEL=qwen3:latest
LLM_BASE_URL=http://localhost:11434
LLM_API_KEY=
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS=2048
LLM_TIMEOUT=300
LLM_MAX_RETRIES=3

# ============ OpenAI Configuration ============
