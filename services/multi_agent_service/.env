# ============================================================
# Multi-Agent Service Configuration
# ============================================================
# Copy this file to .env and modify as needed

# ============ Service Configuration ============
SERVICE_NAME=multi-agent-service
SERVICE_VERSION=1.0.0
SERVICE_HOST=0.0.0.0
HTTP_PORT=8006

# ============ Nacos Configuration ============
NACOS_ENABLED=false
NACOS_SERVER_ADDRESSES=localhost:8848
NACOS_NAMESPACE=public
NACOS_GROUP=DEFAULT_GROUP
NACOS_USERNAME=
NACOS_PASSWORD=

# ============ LLM Configuration (for routing agent) ============
LLM_PROVIDER=ollama
LLM_BASE_URL=http://localhost:11434
LLM_MODEL=qwen3:latest
LLM_API_KEY=
LLM_TEMPERATURE=0.3
LLM_MAX_TOKENS=2048

# ============ A2A Agent Configuration ============
A2A_ENABLED=true
AGENT_HEALTH_CHECK_INTERVAL=30
AGENT_TIMEOUT=60

# ============ Routing Configuration ============
ROUTING_CONFIDENCE_THRESHOLD=0.7
MAX_AGENTS_PER_QUERY=3
ROUTING_FALLBACK_AGENT=general

# ============ Specialized Agents (A2A) ============
# Agent URLs for A2A communication
BILLING_AGENT_URL=http://localhost:9001
TECHNICAL_AGENT_URL=http://localhost:9002
SALES_AGENT_URL=http://localhost:9003
CHARGING_AGENT_URL=http://localhost:9004
GENERAL_AGENT_URL=http://localhost:9005

# ============ Dependent Services ============
SINGLE_AGENT_SERVICE_URL=http://localhost:8005
RAG_SERVICE_URL=http://localhost:8002
MONITORING_SERVICE_URL=http://localhost:8004

# ============ Redis Cache ============
REDIS_URL=redis://localhost:6379/0
REDIS_CACHE_ENABLED=true
REDIS_CACHE_TTL=3600

# ============ Langfuse Observability ============
LANGFUSE_ENABLED=true
LANGFUSE_SECRET_KEY=sk-lf-cd7bcb7d-27ad-4049-b4f7-4f2ca441d579
LANGFUSE_PUBLIC_KEY=pk-lf-dd167d24-a007-4afb-a3af-8c37a922870f
LANGFUSE_HOST=https://cloud.langfuse.com

# ============ Logging Configuration ============
LOG_LEVEL=INFO
LOG_FORMAT=json
LOG_TO_CONSOLE=true
LOG_TO_FILE=true
LOG_DIR=./logs
